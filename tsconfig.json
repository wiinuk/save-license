{
  "compilerOptions": {
    "target": "es6",
    "lib": [
      "es6"
    ],
    "module": "commonjs",
    "declaration": true,
    "outDir": "out",
    "importHelpers": true,
    "downlevelIteration": true,
    "strict": true,
    "noUnusedLocals": true,
    "noUnusedParameters": true,
    "noImplicitReturns": true,
    "noFallthroughCasesInSwitch": true,
    "pretty": true
  },
  "exclude": [
    "node_modules",
    "out"
  ]
}